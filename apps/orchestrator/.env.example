# Execution mode
ORCH_AGENT_EXECUTOR=local
ORCH_CHECK_LLM=false
ORCH_LLM_POLICY=oss
ORCH_LLM_RESPONSE_FORMAT=structured-json
ORCH_PORT=3002
ORCH_WORKER_ENABLED=false
ORCH_WORKER_POLL_INTERVAL_MS=500
ORCH_WORKER_MAX_RETRIES=3
ORCH_WORKER_BACKOFF_MS=2000
ORCH_SCHEDULER_ENABLED=false
ORCH_SCHEDULE_INTERVAL_MS=60000
ORCH_SCHEDULE_ROLES=architect,implementer,reviewer
ORCH_SCHEDULE_DESCRIPTION=Scheduled orchestrator run
ALLOW_HOSTED_LLM=false
LOCAL_LLM_ALLOWED_HOSTS=localhost,127.0.0.1,0.0.0.0,::1
LOCAL_LLM_ERROR_MODE=fail

# Local OSS LLM config
LOCAL_LLM_API=ollama
LOCAL_LLM_URL=http://localhost:11434
LOCAL_LLM_MODEL=llama3.1:8b
LOCAL_LLM_TEMPERATURE=0.2
LOCAL_LLM_TIMEOUT_MS=30000
LOCAL_LLM_MAX_TOKENS=800
# LOCAL_LLM_MODEL_ARCHITECT=
# LOCAL_LLM_MODEL_IMPLEMENTER=
# LOCAL_LLM_MODEL_REVIEWER=
# LOCAL_LLM_MODEL_TESTER=
# LOCAL_LLM_MODEL_MAINTAINER=

# Storage / queue (load secrets via 1Password; do not commit real credentials)
DATABASE_URL=postgresql://<user>:<password>@localhost:5432/<db>
POSTGRES_URL=postgresql://<user>:<password>@localhost:5432/<db>
ORCH_TASK_STORE=postgres
TASK_QUEUE=redis
REDIS_URL=redis://localhost:6379

# Tool runner (optional)
ORCH_TOOL_ALLOWLIST=rg,ls,cat
# ORCH_TOOL_MAX_ITERATIONS=3
# ORCH_TOOL_CWD=
# ORCH_TOOL_ALLOWED_ROOTS=
